{
  "activity": "xc50",
  "split": "random",
  "input_dim": 2048,
  "num_tasks": 20,
  "model_config": {
    "layers": [2048, 256, 256],
    "dropout": 0.1
  },
  "training_config": {
    "batch_size": 128,
    "epochs": 3000,
    "early_stop": 100,

    "optimizer": {
      "type": "adam",
      "lr": 1e-3,
      "weight_decay": 1e-4
    },
    "lr_scheduler": {
      "type": "ReduceLROnPlateau",
      "patience": 20,
      "factor": 0.5
    }
  },
  "optimizer": "adam",
  "loss": "huber"
}