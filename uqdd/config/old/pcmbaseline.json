{
  "chem_input_dim": 2048,
  "prot_input_dim": 768,
  "chem_model_config": {
    "layers": [1024, 512, 256],
    "dropout": 0.2
  },
  "prot_model_config": {
    "layers": [512, 256, 128],
    "dropout": 0.2
  },
  "model_config": {
    "dropout": 0.2
  },
  "regressor_config": {
    "layers": [384, 192]
  },
  "training_config": {
    "batch_size": 128,
    "epochs": 3000,
    "early_stop": 100,
    "loss": "huber",
    "optimizer": {
      "type": "adam",
      "lr": 1e-3,
      "weight_decay": 1e-4
    },
    "lr_scheduler": {
      "type": "ReduceLROnPlateau",
      "patience": 20,
      "factor": 0.5
    }
  }
}